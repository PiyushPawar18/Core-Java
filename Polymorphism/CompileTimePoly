//Compile time Polymorphism
//example method Overloading

class A{
    int a; String name;
    void disp(){
        a=10; name="piyush";
        System.out.println(a+" "+name);
    }

}
class B extends A{

    void disp(int x,String name){
       System.out.println(x+" "+name);
    }
}
class Main{
    public static void main(String[] args){
        B obj=new B();
        obj.disp();               //method overloading //poly
        obj.disp(7,"pawar");
    }
}
